/0001 { /glob "*.html" /type "allow" }
/0002 { /glob "*.css" /type "allow" }
/0003 { /glob "*.js" /type "allow" }
/0004 { /glob "*.gif" /type "allow" }
/0005 { /glob "*.ico" /type "allow" }
/0006 { /glob "*.jpg" /type "allow" }
/0007 { /glob "*.jpeg" /type "allow" }
